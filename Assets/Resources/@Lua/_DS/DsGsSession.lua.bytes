--- Ds在GS上的Session
local DsGsSession = _MOE.class("DsGsSession")


function DsGsSession:Ctor(tb)
    self.tb = tb
end

function DsGsSession:GetDsIp()
    if not self.tb then
        return
    end
    local dsData = self.tb.dsData
    if not dsData then
        return
    end
    return dsData.ip
end

function DsGsSession:GetDsPort()
    if not self.tb then
        return
    end
    local dsData = self.tb.dsData
    if not dsData then
        return
    end
    return dsData.port
end

function DsGsSession:GetGsIp()
    if not self.tb then
        return
    end
    local battlData = self.tb.battleData
    if not battlData then
        return
    end
    return battlData.ip
end

function DsGsSession:GetGsPort()
    if not self.tb then
        return
    end
    local battlData = self.tb.battleData
    if not battlData then
        return
    end
    return battlData.port
end

function DsGsSession:GetDSAPort()
    if not self.tb then
        return
    end
    local dsa = self.tb.dsaData
    if not dsa then
        return
    end
    return dsa.port
end

function DsGsSession:GetDSAIp()
    if not self.tb then
        return
    end
    local dsa = self.tb.dsaData
    if not dsa then
        return
    end
    return dsa.ip
end

function DsGsSession:GetDsToken()
    if not self.tb then
        return
    end
    local dsData = self.tb.dsData
    if not dsData then
        return
    end
    return dsData.token
end

function DsGsSession:GetPlayerInfos()
    if not self.tb then
        return
    end
    return self.tb.playerInfos
end

return DsGsSession