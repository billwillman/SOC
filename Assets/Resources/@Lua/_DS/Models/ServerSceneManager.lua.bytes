local _M = _MOE.class("DsSceneMgr", _MOE.BaseModel)

function _M:OnInit()
    self.ActiveScenes = {}
end

function _M:OnUnInit()
    self:ClearCurrentScene()
end

function _M:ClearCurrentScene()
end

-- 只有异步
function _M:LoadSceneAsync(sceneName)
    if not sceneName then
        return false
    end
    local sceneManager = _MOE.GameApp:GetSceneManager()
    if not sceneManager then
        return false
    end
    
end

return _M