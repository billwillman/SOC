local BaseAppClass = require("Common.BaseApp")

local ClientApp = _MOE.class("ClientApp", BaseAppClass)

function ClientApp:OnInit()
    self:RegisterModel("Client.Models.NetClientModel")
    self:RegisterModel("Client.Models.LoginModel", false)
end

function ClientApp:OnQuit()
    _MOE.UIManager:FreeAllWindow()
    _MOE.NetManager:Disconnect()
end

return ClientApp